#tag ClassProtected Class AlternatingListInherits Listbox	#tag Event		Function CellBackgroundPaint(g As Graphics, row As Integer, column As Integer) As Boolean		  if alternateRows then		    if row mod 2=0 and not me.selected(row) then		      g.foreColor=alternateColor		      g.fillrect 0,0,g.width,g.height		    elseif me.selected(row) then		      g.foreColor=rgb(30,30,100)		      g.fillrect 0,0,g.width,g.height		    end if		  end if		  cellBackgroundPaint(g,row,column)		  return true		End Function	#tag EndEvent	#tag Event		Function CellTextPaint(g As Graphics, row As Integer, column As Integer, x as Integer, y as Integer) As Boolean		  if me.selected(row) then		    g.bold=true		    g.foreColor=&cFFFFFF		  else		    g.bold=false		    g.foreColor=&c000000		  end if		End Function	#tag EndEvent	#tag Hook, Flags = &h0		Event cellBackgroundPaint(g as graphics, row as integer, column as integer)	#tag EndHook	#tag Property, Flags = &h0		alternateColor As color = &cEDF3FF	#tag EndProperty	#tag Property, Flags = &h0		alternateRows As boolean = true	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="alternateColor"			Visible=true			Group="Behavior"			InitialValue="&h000000"			Type="color"		#tag EndViewProperty		#tag ViewProperty			Name="alternateRows"			Visible=true			Group="Behavior"			InitialValue="true"			Type="boolean"		#tag EndViewProperty		#tag ViewProperty			Name="AutoDeactivate"			Visible=true			Group="Appearance"			InitialValue="True"			Type="Boolean"			InheritedFrom="Listbox"		#tag EndViewProperty		#tag ViewProperty			Name="AutoHideScrollbars"			Visible=true			Group="Behavior"			InitialValue="True"			Type="Boolean"			InheritedFrom="Listbox"		#tag EndViewProperty		#tag ViewProperty			Name="Bold"			Visible=true			Group="Font"			Type="Boolean"			InheritedFrom="Listbox"		#tag EndViewProperty		#tag ViewProperty			Name="Border"			Visible=true			Group="Appearance"			InitialVal